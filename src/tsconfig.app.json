      "@flxng/circle-timer/*": [
        "../projects/circle-timer/src/lib/*"
      ],
      "@flxng/loader": [
        "../projects/loader/src/lib"
      ],
      "@flxng/loader/*": [
        "../projects/loader/src/lib/*"
      ],
      "@flxng/paginator": [
        "../projects/paginator/src/lib"
      ],
      "@flxng/paginator/*": [
        "../projects/paginator/src/lib/*"
      ],
      "@flxng/treetable": [
        "../projects/treetable/src/lib"
      ],
      "@flxng/treetable/*": [
        "../projects/treetable/src/lib/*"
      ],
      "@flxng/mentions": [
        "../projects/mentions/src/lib"
      ],
      "@flxng/mentions/*": [
        "../projects/mentions/src/lib/*"
      ]
    }
