{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "baseUrl": "./",
    "module": "esnext",
    "types": [],
    "paths": {
      "@flxng/common": [
        "../projects/common/src/lib"
      ],
      "@flxng/common/*": [
        "../projects/common/src/lib/*"
      ],
      "@flxng/code-show": [
        "../projects/code-show/src/lib"
      ],
      "@flxng/code-show/*": [
        "../projects/code-show/src/lib/*"
